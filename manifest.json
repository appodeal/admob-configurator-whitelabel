{
   "manifest_version": 2,
   "name": "AdMediator",
   "short_name": "AdMediator Admob Sync",
   "description": "This extension will create and sync AdMediator adunits in your Admob account.",
   "version": "1.0",
   "browser_action": {
      "default_icon": {"16": "img/icon/icon-16.png"},
      "default_popup": "popup.html"
   },
   "icons": { "16": "img/icon/icon-16.png", "32": "img/icon/icon-32.png", "64": "img/icon/icon-64.png","128": "img/icon/icon-128.png" },
   "content_scripts": [
      {
         "matches": ["*://*/*"],
         "js": ["js/vendor/jquery.min.js", "js/utils.js", "js/login.js"]
      },
      {
         "matches": [ "https://console.developers.google.com/*" ],
         "js": ["js/vendor/jquery.min.js", "js/utils.js", "js/modal.js"],
         "css": ["css/modal.console.css"]
      },
      {
         "js": ["js/vendor/jquery.min.js","js/utils.js", "js/adunits.js", "js/admob.js","js/admob_v2.js", "js/modal.js", "js/progressBar.js"],
         "matches": [ "https://apps.admob.com/*" ],
         "css": ["css/modal.css"]
      }
   ],
   "background": {
      "scripts": [ "js/vendor/jquery.min.js", "js/utils.js", "js/bg.js"]
   },
   "web_accessible_resources": ["img/*", "js/vendor/*","js/helpers/*"],
    "permissions": [
      "tabs",
      "cookies",
      "storage",
      "notifications",
      "webNavigation",
      "identity",
      "*://*/*",
      "*://*.admob.com/*",
      "*://*.google.com/*"
    ],
    "externally_connectable": {
      "matches": ["https://apps.admob.com/*"]
    }
}